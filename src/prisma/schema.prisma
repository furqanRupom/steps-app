generator client {
    provider        = "prisma-client-js"
    previewFeatures = ["jsonProtocol"]
}

datasource db {
    provider  = "postgresql"
    url       = env("POSTGRES_PRISMA_URL")
    directUrl = env("POSTGRES_URL_NON_POOLING")
}

model Feedback {
    id       String @id @default(uuid())
    name     String
    email    String @unique
    feedback String

    @@map("feedbacks")
}
